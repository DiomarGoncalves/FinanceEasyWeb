{
  "name": "FinancEasy",
  "version": "0.0.2",
  "description": "FinancEasy é uma aplicação de gerenciamento financeiro pessoal que permite aos usuários controlar suas despesas e receitas, gerar relatórios detalhados e calcular o saldo atual. Desenvolvido com Electron, a aplicação oferece uma interface amigável e intuitiva para facilitar o acompanhamento das finanças pessoais.",
  "author": "Diomar Gonçalves",
  "main": "public/main.js",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "build:tailwind": "npx tailwindcss -i ./src/input.css -o ./public/styles.css --watch"
  },
  "devDependencies": {
    "@electron-forge/cli": "^7.6.1",
    "@electron-forge/maker-deb": "^7.6.1",
    "@electron-forge/maker-rpm": "^7.6.1",
    "@electron-forge/maker-squirrel": "^7.6.1",
    "@electron-forge/maker-zip": "^7.6.1",
    "@electron-forge/plugin-auto-unpack-natives": "^7.6.1",
    "@electron-forge/plugin-fuses": "^7.6.1",
    "@electron/fuses": "^1.8.0",
    "autoprefixer": "^10.4.15",
    "electron": "^26.2.4",
    "electron-reload": "^2.0.0-alpha.1",
    "postcss": "^8.4.27",
    "tailwindcss": "^3.4.1"
  },
  "dependencies": {
    "bootstrap": "^5.3.3",
    "chart.js": "^3.5.1",
    "dotenv": "^16.5.0",
    "electron-squirrel-startup": "^1.0.1",
    "express": "^4.21.2",
    "framer-motion": "^10.16.1",
    "jspdf": "^2.4.0",
    "jspdf-autotable": "^3.5.13",
    "lucide-react": "^0.276.0",
    "pg": "^8.14.1",
    "save-dev": "^0.0.1-security",
    "sqlite3": "^5.1.7"
  }
}
